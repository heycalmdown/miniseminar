{
  "name": "miniseminar",
  "version": "1.7.2",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "tsc",
    "test": "jasmine lib/spec/**/*.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "conventional-github-releaser -p angular"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "confluency": "^0.9.0",
    "cookie-parser": "^1.4.3",
    "debug": "^4.0.0",
    "express": "^4.16.3",
    "lodash": ">=4.17.11",
    "morgan": "^1.9.0",
    "pug": "^2.0.3",
    "reveal.js": "^3.7.0",
    "serve-favicon": "^2.5.0",
    "superagent": "^5.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cheerio": "^0.22.9",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/jasmine": "^3.0.0",
    "@types/lodash": "^4.14.116",
    "@types/morgan": "^1.7.35",
    "conventional-changelog-cli": "^2.0.0",
    "conventional-github-releaser": "^3.0.0",
    "husky": "^3.0.0",
    "jasmine": "^3.2.0",
    "typescript": "^3.0.1",
    "validate-commit-msg": "^2.11.1"
  },
  "config": {
    "validate-commit-msg": {
      "scope": {
        "allowed": [
          ".ignore",
          "readme",
          "changelog",
          "package",
          "docker",
          "codefactor",
          "test",
          "app",
          "img",
          "code",
          "plugin",
          "home"
        ],
        "validate": true
      },
      "autoFix": true
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg"
    }
  }
}
