{
  "name": "miniseminar",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "npm run babel && jasmine dist/spec/**/*.js",
    "babelstart": "npm run babel && npm start",
    "babel": "babel -d dist/ src/",
    "commitmsg": "validate-commit-msg",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "conventional-github-releaser -p angular"
  },
  "dependencies": {
    "body-parser": "~1.17.0",
    "cheerio": "~0.22.0",
    "confluency": "^0.4.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.1",
    "express": "~5.0.0-alpha.5",
    "lodash": "^4.17.4",
    "morgan": "~1.8.1",
    "pug": "^2.0.0-beta11",
    "superagent": "^3.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.1",
    "conventional-changelog": "^1.1.0",
    "conventional-github-releaser": "^1.1.3",
    "husky": "^0.13.1",
    "jasmine": "^2.5.3",
    "validate-commit-msg": "^2.11.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "config": {
    "validate-commit-msg": {
      "scope": {
        "allowed": [
          ".ignore",
          "readme",
          "changelog",
          "package",
          "docker",
          "codefactor",
          "app",
          "img",
          "code",
          "home"
        ],
        "validate": true
      },
      "autoFix": true
    }
  }
}
